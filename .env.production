# 生产环境配置
NODE_ENV=production
PORT=40000

# 服务器配置
# CORS 设置为 * 允许所有来源（如果需要安全限制，改为特定来源）
CORS_ORIGIN=*
DATA_PATH=./server/GD
MAX_UPLOAD_SIZE=10485760
MAX_BATCH_SIZE=30000

# 日志配置
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# 安全配置 - 不启用 HSTS 以支持 HTTP
ENABLE_HSTS=false

# 前端配置 - 使用相对路径以支持 HTTP/HTTPS 自适应
VITE_API_BASE_URL=/api/v1
VITE_MAX_BATCH_SIZE=30000
VITE_MAX_FILE_SIZE=10485760
